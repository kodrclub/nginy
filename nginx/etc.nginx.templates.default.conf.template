server {
    listen       80;
    listen  [::]:80;
    server_name  localhost;

    #charset koi8-r;
    #access_log  /var/log/nginx/host.access.log  main;

    location / {
        root   /usr/share/nginx/html;
        index  index.html index.htm;
    }

    location /one {
        rewrite /one/(.*) /$1/  break;
        proxy_pass http://web1:8000/;
    }

    location /two {
        rewrite /two/(.*) /$1/  break;
        proxy_pass http://web2:8000/;
    }



}

